mongo taker --eval "db.createCollection('ex_usd_try')"
mongo taker --eval "db.ex_usd_try.createIndex({'type': 1}, {'unique': false})"
mongo taker --eval "db.ex_usd_try.createIndex({'timestamp': 1}, {'unique': false})"

